<p id="notice"><%= notice %></p>

<h1>Listing Pre Challenges</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th colspan="3"></th>
      <th>Current_state</th>
      <th>Industry name</th>
    </tr>
  </thead>

  <tbody>
    <% @pre_challenges.each do |pre_challenge| %>
      <tr>
        <td><%= pre_challenge.name %></td>
        <td><%= link_to 'Show', pre_challenge %></td>
        <td><%= link_to 'Edit', edit_pre_challenge_path(pre_challenge) %></td>
        <td><%= link_to 'Destroy', pre_challenge, method: :delete, data: { confirm: 'Are you sure?' } %></td>
        <td><%= pre_challenge.current_state %></td>
        <td><%= pre_challenge.industry.name %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Pre challenge', new_pre_challenge_path %>
