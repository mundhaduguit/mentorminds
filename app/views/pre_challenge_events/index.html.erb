<p id="notice"><%= notice %></p>

<h1>Listing Pre Challenge Events</h1>

<table>
  <thead>
    <tr>
      <th>State</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @pre_challenge_events.each do |pre_challenge_event| %>
      <tr>
        <td><%= pre_challenge_event.state %></td>
        <td><%= link_to 'Show', pre_challenge_event %></td>
        <td><%= link_to 'Edit', edit_pre_challenge_event_path(pre_challenge_event) %></td>
        <td><%= link_to 'Destroy', pre_challenge_event, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Pre challenge event', new_pre_challenge_event_path %>
